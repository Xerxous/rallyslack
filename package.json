{
  "name": "rallyslack",
  "version": "1.0.1",
  "description": "The Rally Slackbot is a utility for Slack users to quickly access and modify Rally.",
  "main": "index.js",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "botkit": "^0.2.2",
    "fs": "^0.0.2",
    "jsonfile": "^2.3.1",
    "lodash": "^4.17.4",
    "nodemon": "^1.12.1",
    "rally": "^0.2.0",
    "replace": "^0.3.0",
    "request": "^2.73.0",
    "text-table": "^0.2.0",
    "util": "^0.10.3",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "eslint": "^3.0.0"
  },
  "scripts": {
    "start": "nodemon index.js --exec babel-node --ignore '*.json'",
    "compile": "babel . --ignore node_modules,README.md --out-dir .out --copy-files",
    "prod": "forever start ./.out/app.js -e err.log"
